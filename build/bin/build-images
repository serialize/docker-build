#!/bin/bash
. $(dirname $0)/set-env

$BUILD_IMG_BIN base default
$BUILD_IMG_BIN base php
$BUILD_IMG_BIN base go
$BUILD_IMG_BIN base mono

$BUILD_IMG_BIN devel default
$BUILD_IMG_BIN devel php
$BUILD_IMG_BIN devel go
$BUILD_IMG_BIN devel mono

$BUILD_IMG_BIN srv dnsmasq
$BUILD_IMG_BIN srv etcd
$BUILD_IMG_BIN srv pgsql
$BUILD_IMG_BIN srv mariadb 
$BUILD_IMG_BIN srv nginx
$BUILD_IMG_BIN srv nodejs

$BUILD_IMG_BIN app openerp
$BUILD_IMG_BIN app owncloud
